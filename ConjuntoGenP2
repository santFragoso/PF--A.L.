A = matrix(QQ,2,4,[1,2,-1,3,3,5,-4,7])
B = identity_matrix(4)
print(A)
m,n = A.dimensions()
print(" ")
AER = A.rref()
print(AER)
Mayor = max(m,n)
AN =matrix(Mayor,Mayor)
print(" ")

for i in range (0,Mayor):
    for j in range (0,Mayor):
        if (i<m):
            AN[i,j]=A[i][j]
        if (i>m):
            AN[i,j]=0
print(AN)
print(" ")
ARN = AN.rref()
print(ARN)
CG = ARN - B
print(" ")
print(CG)

print(-1*CG[][1])
